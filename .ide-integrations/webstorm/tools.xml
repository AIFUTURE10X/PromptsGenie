<?xml version="1.0" encoding="UTF-8"?>
<toolSet name="MCP Tools">
  <tool name="MCP: Verify Configuration" description="Verify MCP configuration and setup" showInMainMenu="true" showInEditor="true" showInProject="true" showInSearchPopup="true" disabled="false" useConsole="true" showConsoleOnStdOut="true" showConsoleOnStdErr="true" synchronizeAfterRun="true">
    <exec>
      <option name="COMMAND" value="node" />
      <option name="PARAMETERS" value=".trae/verify-mcp.js" />
      <option name="WORKING_DIRECTORY" value="$ProjectFileDir$" />
    </exec>
  </tool>
  
  <tool name="MCP: Start Vision Tool" description="Start the Vision Gemini MCP tool" showInMainMenu="true" showInEditor="true" showInProject="true" showInSearchPopup="true" disabled="false" useConsole="true" showConsoleOnStdOut="true" showConsoleOnStdErr="true" synchronizeAfterRun="true">
    <exec>
      <option name="COMMAND" value="node" />
      <option name="PARAMETERS" value="tools/vision-gemini.js" />
      <option name="WORKING_DIRECTORY" value="$ProjectFileDir$" />
    </exec>
  </tool>
  
  <tool name="MCP: Validate Filesystem Config" description="Validate filesystem configuration" showInMainMenu="true" showInEditor="true" showInProject="true" showInSearchPopup="true" disabled="false" useConsole="true" showConsoleOnStdOut="true" showConsoleOnStdErr="true" synchronizeAfterRun="true">
    <exec>
      <option name="COMMAND" value="node" />
      <option name="PARAMETERS" value="-e &quot;console.log(JSON.stringify(require('./.trae/filesystem-config.json'), null, 2))&quot;" />
      <option name="WORKING_DIRECTORY" value="$ProjectFileDir$" />
    </exec>
  </tool>
  
  <tool name="MCP: Check Git Configuration" description="Check Git configuration for MCP" showInMainMenu="true" showInEditor="true" showInProject="true" showInSearchPopup="true" disabled="false" useConsole="true" showConsoleOnStdOut="true" showConsoleOnStdErr="true" synchronizeAfterRun="true">
    <exec>
      <option name="COMMAND" value="node" />
      <option name="PARAMETERS" value="-e &quot;console.log(JSON.stringify(require('./.trae/git-config.json'), null, 2))&quot;" />
      <option name="WORKING_DIRECTORY" value="$ProjectFileDir$" />
    </exec>
  </tool>
  
  <tool name="MCP: Install Dependencies" description="Install MCP tool dependencies" showInMainMenu="true" showInEditor="true" showInProject="true" showInSearchPopup="true" disabled="false" useConsole="true" showConsoleOnStdOut="true" showConsoleOnStdErr="true" synchronizeAfterRun="true">
    <exec>
      <option name="COMMAND" value="npm" />
      <option name="PARAMETERS" value="install @google/generative-ai" />
      <option name="WORKING_DIRECTORY" value="$ProjectFileDir$" />
    </exec>
  </tool>
</toolSet>